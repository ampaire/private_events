<div class="row">
  <div class="col-md-6 col-md-offset-3">
  <h1>LOG IN</h1>
    <%= form_for(:session, url: login_path) do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>

    <%= f.submit "Sign in", class: "btn btn-primary" %>
    <% end %>
    <p> New user?    <%= link_to "SignUp", signup_path %> </p>
  </div>
</div>